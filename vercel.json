{
  "version": 2,
  "builds": [
    {
      "src": "api/mediafire.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/pinterest.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/mediafire",
      "dest": "/api/mediafire.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/pinterest",
      "dest": "/api/pinterest.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
