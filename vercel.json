{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/pinterest",
      "destination": "/api/pinterest.js"
    },
    {
      "source": "/api/mediafire",
      "destination": "/api/mediafire.js"
    }
  ]
}
